<tr>
  <td class="p-4 border-b border-slate-100"><%= set.scheduled_date.strftime("%A at %l:%M %P") %></td>
  <td class="p-4 border-b border-slate-100">
    <% if set.set_drivers.length > 0 %>
      <%= set.set_drivers[0].admin.first_name %>
      <% if set.set_drivers.length > 1 %>
        <%= "and " + set.set_drivers[1].admin.first_name %>
      <% end %>
    <% end %>
  </td>
  <td class="p-4 border-b border-slate-100"><%= set.dib_limit - set.dib_count %></td>
  <td class="p-4 border-b border-slate-100"><%= set.chib_limit - set.chib_count %></td>
  <td class="p-4 border-b border-slate-100"><%= link_to "View", set, class: "rounded bg-slate-300 border border-slate-400 px-3 py-2 drop-shadow" %></td>
</tr>